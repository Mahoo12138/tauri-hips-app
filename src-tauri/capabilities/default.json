{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Capability for the main window",
  "windows": [
    "main"
  ],
  "permissions": [
    "core:default",
    "opener:default",
    "store:default",
    "store:allow-load",
    {
      "identifier": "http:default",
      "allow": [
        {
          "url": "https://*.typicode.com"
        }
      ],
      "deny": [
        {
          "url": "https://private.tauri.app"
        }
      ]
    },
    "notification:default",
    "store:default"
  ],
  "capabilities": []
}